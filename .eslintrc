{
    "extends": "@steroidsjs/eslint-config",
    "rules": {
      "no-undef": "off",
      "no-redeclare": "off",
      "react/no-unused-prop-types": "off",
      "react/default-props-match-prop-types": "off",
      "react/sort-comp": "off",
      "import/no-extraneous-dependencies": ["error", {
        "devDependencies": [
        "**/*.test.js",
        "**/*.spec.js",
        "**/setupTests.js",
        "**/*.config.js"
      ]
      }],
      "indent": ["error", 4]
    }
  }
